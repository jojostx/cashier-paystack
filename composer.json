{
    "name": "jojostx/cashier-paystack",
    "description": "A Laravel Cashier Wrapper that provides an expressive, fluent interface to Paystack's subscription invoicing services.",
    "keywords": ["laravel", "paystack", "billing", "cashier"],
    "license": "MIT",
    "support": {
        "source": "https://github.com/jojostx/cashier-paystack"
    },
    "authors": [
        {
            "name": "Ikuru John",
            "email": "ikuskid7@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "ext-json": "*",
        "dompdf/dompdf": "^0.8.0",
        "illuminate/console": "~8|~9",
        "illuminate/contracts": "~8|~9",
        "illuminate/database": "~8|~9",
        "illuminate/http": "~8|~9",
        "illuminate/log": "~8|~9",
        "illuminate/notifications": "~8|~9",
        "illuminate/routing": "~8|~9",
        "illuminate/support": "~8|~9",
        "nesbot/carbon": "^1.26.3|^2.0",
        "symfony/http-kernel": "^5.0|^6.0",
        "unicodeveloper/laravel-paystack": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Jojostx\\Cashier\\Paystack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jojostx\\Cashier\\Paystack\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
